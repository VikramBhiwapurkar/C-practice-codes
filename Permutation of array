#include <bits/stdc++.h>
using namespace std;
vector<vector<int>>ans;
void permute(vector<int>arr, int index){
    if(index==arr.size()){
        ans.push_back(arr);
        return;
    }
    
    for(int i=index;i<arr.size();i++){
        swap(arr[index],arr[i]);
        permute(arr, index+1);
        swap(arr[index], arr[i]);   //backtracking
    }
}
int main() {
	// your code goes here
    int n;
    cin>>n;
    
    vector<int>arr;
    for(int i=0;i<n;i++){
        int temp;
        cin>>temp;
        arr.push_back(temp);
    }
    permute(arr, 0);
    for(int i=0;i<ans.size();i++){
        for(int j=0;j<ans[0].size();j++){
            cout<<ans[i][j]<<" ";
        }
        cout<<endl;
    }
}
